{
  "version": 2,
  "builds": [
    {
      "src": "comedy_shows/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "comedy_shows/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "comedy_shows.settings"
  }
}
